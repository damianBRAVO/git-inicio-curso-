package estructuras.repetivas.pkgwhile;

/**
 *
 * @author PC
 */
public class EstructurasRepetivasWHILE {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
    
        int cont = 0 ;
        while (cont <=10){
            System.out.println("Estoy en la vuelta"+cont);
            cont = cont+1;
        }
    }
    
}
