/*
 Realizar un programa que dado por teclado un límite numérico por teclado (por ejemplo 100) muestre en pantalla todos los números hasta ese límite (empezando por 1).
 */

package numeroslimites;
import java.util.Scanner;

/**
 * @author damianBRAVO
 */
public class NumerosLimites {

    public static void main(String[] args) {
    //tenemos un límite hasta el que tenemos que contar
    // ingrese ese límite
        System.out.println("Ingrese ese limite hasta el que quiere contar");
        Scanner teclado = new Scanner (System.in);
        int limite = teclado.nextInt();
        int cont = 1;
        while (cont <= limite  ){
            System.out.println(cont);
            cont = cont + 1;
        }
        
    }
    
}
