
package matrices;

/**
 * @author damianBRAVO
 */
public class Matrices {

   
    public static void main(String[] args) {
        // Declaracion
        int matriz[][]= new int [3][3];
         // Asignacion (Manual)
         matriz[0][0]=5;
         matriz[0][1]=13;
         matriz[2][2]=24;
        
    }
    
}
